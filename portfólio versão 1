                <div class="slider-dots">
                  <span class="dot active" onclick="testimonialSlider.goToSlide(0)"></span>
                  <span class="dot" onclick="testimonialSlider.goToSlide(1)"></span>
                  <span class="dot" onclick="testimonialSlider.goToSlide(2)"></span>
                  <span class="dot" onclick="testimonialSlider.goToSlide(3)"></span>
                </div>
              </div>
            </div>
