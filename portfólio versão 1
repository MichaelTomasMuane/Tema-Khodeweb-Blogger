       <!-- =================================================================
       CABEÇALHO DAS PÁGINAS DE ARQUIVO E PESQUISA
       ================================================================= -->
        <b:section class='archive-header-area' id='archive-header' maxwidgets='1' name='Archive Header' showaddelement='false'>
          <b:widget id='HTML102' locked='true' title='Archive Page Header' type='HTML' version='2' visible='true'>
            <b:widget-settings>
              <b:widget-setting name='content'/>
            </b:widget-settings>
            <b:includable id='main'>
              <div class='archive-header-wrapper'>
                <div class='overlay'/>
                <div class='archive-header-content'>
                  <h1 class='archive-title'>
                    <b:if cond='data:view.isLabelSearch'>
                      Etiqueta: <data:view.search.label/>
                    <b:elseif cond='data:view.isArchive'/>
                      Arquivo de Artigos
                    <b:elseif cond='data:view.Search'/>
                      Resultados para: <data:view.search.query/>
                    <b:else/>
                      <b:elseif cond='data:view.Search'/>
                      Resultados para: <data:view.search.label/>
                    <b:else/>
                      Artigos
                    </b:if>
                  </h1>
                  <div class='archive-breadcrumbs'>
                    <a expr:href='data:blog.homepageUrl'>Home</a> &#9733;
                    <a href='/search/'>Artigos</a> &#9733;
                    <span class='current'><data:view.search.label/></span>
                  </div>
                </div>
              </div>
            </b:includable>
          </b:widget>
        </b:section>
	
		<!-- =================================================================
        CONTEÚDO PRINCIPAL (LAYOUTS CONDICIONAIS)
        ================================================================= -->
        <div class='main-content-wrapper'>
          <b:section class='main' id='main-content' name='Main Content' showaddelement='true'>
            <b:widget id='Blog1' locked='true' title='Mensagens de blogue' type='Blog' version='2' visible='true'>
              <b:widget-settings>
                <b:widget-setting name='showDateHeader'>false</b:widget-setting>
                <b:widget-setting name='commentLabel'>Comments</b:widget-setting>
