          <script type='text/javascript'>
          //<![CDATA[
            const testimonialSlider = {
              slides: document.querySelectorAll('.testimonial-slide'),
              dots: document.querySelectorAll('.slider-dots .dot'),
              currentIndex: 0,
              intervalId: null,

              showSlide: function(index) {
                this.slides.forEach((slide, i) => {
                  slide.classList.remove('active');
                  this.dots[i].classList.remove('active');
                });
                this.slides[index].classList.add('active');
                this.dots[index].classList.add('active');
                this.currentIndex = index;
              },

              nextSlide: function() {
                let nextIndex = (this.currentIndex + 1) % this.slides.length;
                this.showSlide(nextIndex);
              },

              start: function() {
                // Inicia mostrando o primeiro slide.
                this.showSlide(0);
                // Configura o intervalo para mudar de slide a cada 9 segundos (10000 ms).
                this.intervalId = setInterval(() => this.nextSlide(), 10000);
              },

              // Função para quando o utilizador clica num dos pontos de navegação.
              goToSlide: function(index) {
                this.showSlide(index);
                // Limpa o intervalo antigo e reinicia para que o próximo slide não mude demasiado depressa.
                clearInterval(this.intervalId);
                this.intervalId = setInterval(() => this.nextSlide(), 8000);
              }
            };

            // Inicia o slider quando a página carrega.
            testimonialSlider.start();
          //]]]]><![CDATA[>
          </script>]]></b:widget-setting>
        </b:widget-settings>
        <b:includable id='main'>
            <b:include name='widget-title'/>
            <div class='widget-content'>
              <data:content/>
            </div>
          </b:includable>
      </b:widget>
    </b:section>
        </b:if>
