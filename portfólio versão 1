              <script type='text/javascript'>
              //<![CDATA[
                (function() {
                  const faqItems = document.querySelectorAll('.faq-accordion .faq-item');

                  faqItems.forEach(item => {
                    const question = item.querySelector('.faq-question');
                    question.addEventListener('click', () => {
                      // Encontra o item que está atualmente ativo
                      const currentlyActive = document.querySelector('.faq-accordion .faq-item.active');

                      // Se houver um item ativo e não for o que acabamos de clicar, fecha-o
                      if (currentlyActive && currentlyActive !== item) {
                        currentlyActive.classList.remove('active');
                      }

                      // Abre ou fecha o item clicado
                      item.classList.toggle('active');
                    });
                  });
                })();
              //]]]]><![CDATA[>
              </script>]]></b:widget-setting>
