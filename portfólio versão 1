    <script src='https://cdn.jsdelivr.net/npm/basiclightbox@5.0.4/dist/basicLightbox.min.js'/>
    
    <script type='text/javascript'>
    //<![CDATA[
    // Lógica para ativar a Lightbox nas imagens dos artigos
    document.addEventListener('DOMContentLoaded', function() {
      // Executa o código apenas se estivermos numa página de artigo
      if (document.body.classList.contains('isSingleItem')) {

        // Seleciona todas as imagens dentro do corpo do post
        const postImages = document.querySelectorAll('.single-post-body img');

        postImages.forEach(function(image) {
          // Adiciona um estilo para o cursor indicar que a imagem é clicável
          image.style.cursor = 'pointer';

          // Adiciona o evento de clique a cada imagem
          image.addEventListener('click', function(e) {
            // Previne qualquer comportamento padrão (caso a imagem seja um link)
            e.preventDefault();

            // Obtém o URL da imagem clicada
            const imageUrl = this.src;

            // Cria e mostra a lightbox com a imagem em alta resolução
            basicLightbox.create(`
                <img src="${imageUrl}" style="max-width: 90vw; max-height: 90vh;"/>
            `).show();
          });
        });
      }
    });
    //]]>
    </script>
    
    <script type='text/javascript'>
    //<![CDATA[
    (function() {
        var hamburger = document.querySelector('.hamburger-icon');
        var sidebar = document.querySelector('.sidebar-container');
        var overlay = document.querySelector('.dim-overlay');
        var body = document.body;
        var closeButton = document.querySelector('.sidebar-back');

        function toggleSidebar() {
            body.classList.toggle('sidebar-visible');
            sidebar.classList.toggle('sidebar-invisible');
            overlay.style.display = body.classList.contains('sidebar-visible') ? 'block' : 'none';
        }

        if (hamburger && sidebar && overlay && closeButton) {
            hamburger.addEventListener('click', toggleSidebar);
            overlay.addEventListener('click', toggleSidebar);
            closeButton.addEventListener('click', toggleSidebar);
        }
    })();
    //]]>
    </script>
    
    <script type='text/javascript'>
    //<![CDATA[
        (function(w,d,e,u,f,l,n){w[f]=w[f]||function(){(w[f].q=w[f].q||[])
          .push(arguments);},l=d.createElement(e),l.async=1,l.src=u,
          n=d.getElementsByTagName(e)[0],n.parentNode.insertBefore(l,n);})
          (window,document,'script','https://assets.mailerlite.com/js/universal.js','ml');
          ml('account', '1817069');
        })();
    //]]>
	</script>
    
    <script type='text/javascript'>
    //<![CDATA[
    // Lógica FINAL E CORRIGIDA para Posts Relacionados com Imagens Nítidas
    function showRelatedPosts(json) {
        const container = document.getElementById('related-posts-grid');
        if (!container) return;

        let output = '';
        const entries = json.feed.entry || [];
        let postsShown = 0;
        const placeholderImage = 'https://i.imgur.com/NotImage.png';

        for (let i = 0; i < entries.length; i++) {
            if (postsShown >= related_post_config.num_posts) break;

            const entry = entries[i];
            const postUrl = entry.link.find(link => link.rel === 'alternate').href;

            if (postUrl === related_post_config.current_post_url) continue;

            const postTitle = entry.title.$t;
            let postImage = placeholderImage;

            // ================== A SOLUÇÃO FINAL E CORRETA ESTÁ AQUI ==================
            let imageUrlSource = null;

            // Prioridade 1: Tenta obter a imagem do conteúdo do post.
            if ('content' in entry && entry.content.$t) {
                const tempDiv = document.createElement('div');
                tempDiv.innerHTML = entry.content.$t;
                const firstImg = tempDiv.querySelector('img');
                if (firstImg) {
                    imageUrlSource = firstImg.src;
                }
            }

            // Prioridade 2 (Fallback): Se não houver imagem no conteúdo, usa a miniatura.
            if (!imageUrlSource && 'media$thumbnail' in entry) {
                imageUrlSource = entry.media$thumbnail.url;
            }

            if (imageUrlSource) {
                // Esta é a correção. Substitui qualquer parâmetro de tamanho (/s320/, /w640/, /s72-c/) por /s600/
                postImage = imageUrlSource.replace(/\/(s|w)\d+(-[a-z]+)*\//, '/s600/');
            }
            // =======================================================================

            output += `
                <div class="related-post-card">
                    <a class="post-image-wrapper" href="${postUrl}">
                        <img src="${postImage}" alt="${postTitle}" loading="lazy"/>
                    </a>
                    <div class="post-content">
                        <h4><a href="${postUrl}">${postTitle}</a></h4>
                    </div>
                </div>
            `;
            postsShown++;
        }
        container.innerHTML = output;
    }

    // O código que chama o feed permanece o mesmo
    if (typeof related_post_config !== 'undefined' && related_post_config.first_label) {
        const feedUrl = `/feeds/posts/default/-/${encodeURIComponent(related_post_config.first_label)}?alt=json-in-script&max-results=${related_post_config.num_posts + 1}&callback=showRelatedPosts`;
        const script = document.createElement('script');
        script.src = feedUrl;
        document.body.appendChild(script);
    }
    //]]>
    </script>
    
   <!-- RASTREADOR KHODEWEB (LOCALSTORAGE) -->
    <script>
      //<![CDATA[
        (function() {
          // 1. Tenta ler o ?ref= da URL
          var params = new URLSearchParams(window.location.search);
          var refCode = params.get("ref");

          // 2. Se encontrar, guarda na memória do navegador (LocalStorage)
          // Isso fica gravado mesmo se a pessoa fechar o navegador e voltar depois.
          if (refCode) {
            localStorage.setItem("khodeweb_saved_ref", refCode);
            console.log("Referência guardada: " + refCode);
          }
        })();
      //]]>
    </script>
    
  </body>
</html>
